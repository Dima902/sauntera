{
  "expo": {
    "jsEngine": "hermes",
    "name": "Sauntera",
    "slug": "sauntera",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sauntera",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to suggest better date ideas."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sauntera.app",
      "permissions": ["ACCESS_FINE_LOCATION", "READ_CALENDAR", "WRITE_CALENDAR"],
      "versionCode": 66,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "category": ["BROWSABLE", "DEFAULT"],
          "data": [{ "scheme": "sauntera", "host": "*" }]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#79BAEC",
          "imageWidth": 100
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "kotlinVersion": "2.0.21",
            "gradlePluginVersion": "8.8.2",
            "extraMavenRepos": ["https://jitpack.io", "https://maven.revenuecat.com"]
          }
        }
      ],
      "@react-native-firebase/app",
      "expo-font",
      "expo-localization",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": { "origin": false },
      "firebase": {
        "apiKey": "AIzaSyA-BuWXUPJgZ_ku0Y08Uy_Hse12jsUQNsE",
        "authDomain": "happoria.firebaseapp.com",
        "projectId": "happoria",
        "storageBucket": "happoria.firebasestorage.app",
        "messagingSenderId": "450911057083",
        "appId": "1:450911057083:web:836cf8365620ebc05c659e",
        "measurementId": "G-BZN9DB7P8R"
      },
      "eas": {
        "projectId": "027caa27-2347-47ef-99ff-e9c1eea45132"
      }
    },
    "owner": "dimonchik2011"
  }
}
